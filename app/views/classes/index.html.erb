<p id='notice'><% notice %></p>
<br>
<div class="container">
<h2>Classes</h2>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Name</th>
      </tr>
    </thead>
    <tbody>
      <% @all_classes.each do |classes| %>
        <tr>
          <td><%= link_to classes.name, class_path(:id => classes.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>